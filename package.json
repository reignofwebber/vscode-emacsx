{
	"publisher": "reignofwebber",
	"name": "emacsx",
	"repository": "https://github.com/reignofwebber/vscode-emacsx.git",
	"displayName": "Emacs<>Vscode",
	"description": "Emacs keymap for vscode",
	"version": "0.0.1",
	"icon": "emacs.png",
	"engines": {
		"vscode": "^1.33.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"keybindings": [
			{
				"command": "C-f",
				"key": "ctrl+f",
				"when": "editorTextFocus"
			},
			{
				"command": "C-b",
				"key": "ctrl+b",
				"when": "editorTextFocus"
			},
			{
				"command": "C-n",
				"key": "ctrl+n",
				"when": "editorTextFocus"
			},
			{
				"command": "C-p",
				"key": "ctrl+p",
				"when": "editorTextFocus"
			},
			{
				"command": "M-f",
				"key": "alt+f",
				"when": "editorTextFocus"
			},
			{
				"command": "M-b",
				"key": "alt+b",
				"when": "editorTextFocus"
			},
			{
				"command": "C-a",
				"key": "ctrl+a",
				"when": "editorTextFocus"
			},
			{
				"command": "C-e",
				"key": "ctrl+e",
				"when": "editorTextFocus"
			},
			{
				"command": "M-<",
				"key": "shift+alt+,",
				"when": "editorTextFocus"
			},
			{
				"command": "M->",
				"key": "shift+alt+.",
				"when": "editorTextFocus"
			},
			{
				"command": "M-l",
				"key": "alt+l",
				"when": "editorTextFocus"
			},
			{
				"command": "C-v",
				"key": "ctrl+v",
				"when": "editorTextFocus"
			},
			{
				"command": "M-v",
				"key": "alt+v",
				"when": "editorTextFocus"
			},
			{
				"command": "C-l",
				"key": "ctrl+l",
				"when": "editorTextFocus"
			},
			{
				"command": "C-d",
				"key": "ctrl+d",
				"when": "editorTextFocus"
			},
			{
				"command": "C-k",
				"key": "ctrl+k",
				"when": "editorTextFocus"
			},
			{
				"command": "M-d",
				"key": "alt+d",
				"when": "editorTextFocus"
			},
			{
				"command": "M-del",
				"key": "alt+backspace",
				"when": "editorTextFocus"
			},
			{
				"command": "C-w",
				"key": "ctrl+w",
				"when": "editorTextFocus"
			},
			{
				"command": "M-w",
				"key": "alt+w",
				"when": "editorTextFocus"
			},
			{
				"command": "C-y",
				"key": "ctrl+y",
				"when": "editorTextFocus"
			},
			{
				"command": "M-z",
				"key": "alt+z",
				"when": "editorTextFocus"
			},
			{
				"command": "M-y",
				"key": "alt+y",
				"when": "editorTextFocus"
			},
			{
				"command": "C-j",
				"key": "ctrl+j",
				"when": "editorTextFocus"
			},
			{
				"command": "C-m",
				"key": "ctrl+m",
				"when": "editorTextFocus"
			},
			{
				"command": "M-g",
				"key": "alt+g",
				"when": ""
			},
			{
				"command": "C-o",
				"key": "ctrl+o",
				"when": "editorTextFocus"
			},
			{
				"command": "C-/",
				"key": "ctrl+/",
				"when": "editorTextFocus"
			},
			{
				"command": "C-?",
				"key": "ctrl+shift+/",
				"when": "editorTextFocus"
			},
			{
				"command": "C-g",
				"key": "ctrl+g",
				"when": "editorTextFocus"
			},
			{
				"command": "C-c",
				"key": "ctrl+c",
				"when": "editorTextFocus"
			},
			{
				"command": "C-u",
				"key": "ctrl+u",
				"when": "editorTextFocus"
			},
			{
				"command": "C-s",
				"key": "ctrl+s",
				"when": "editorTextFocus"
			},
			{
				"command": "C-r",
				"key": "ctrl+r",
				"when": "editorTextFocus"
			},
			{
				"command": "M-s",
				"key": "alt+s",
				"when": "editorTextFocus"
			},
			{
				"command": "M-r",
				"key": "alt+r",
				"when": "editorTextFocus"
			},
			{
				"command": "C-;",
				"key": "ctrl+;",
				"when": "editorTextFocus"
			},
			{
				"command": "C-z",
				"key": "ctrl+z",
				"when": "editorTextFocus"
			},
			{
				"command": "C-q",
				"key": "ctrl+q",
				"when": "editorTextFocus"
			},
			{
				"command": "M-m",
				"key": "alt+m",
				"when": "editorTextFocus"
			},
			{
				"command": "M-\\",
				"key": "alt+\\",
				"when": "editorTextFocus"
			},
			{
				"command": "M-j",
				"key": "alt+j",
				"when": "editorTextFocus"
			},
			{
				"command": "__Del__",
				"key": "backspace",
				"when": "editorTextFocus"
			},
			{
				"command": "C-x",
				"key": "ctrl+x",
				"when": ""
			},
			{
				"command": "C-Spc",
				"key": "ctrl+space",
				"when": "editorTextFocus"
			},
			{
				"native": true,
				"command": "workbench.action.showCommands",
				"key": "alt+x"
			},
			{
				"native": true,
				"command": "workbench.action.nextEditor",
				"key": "alt+]"
			},
			{
				"native": true,
				"command": "workbench.action.previousEditor",
				"key": "alt+["
			},
			{
				"native": true,
				"command": "workbench.action.nextPanelView",
				"key": "alt+]",
				"when": "panelFocus"
			},
			{
				"native": true,
				"command": "workbench.action.previousPanelView",
				"key": "alt+[",
				"when": "panelFocus"
			},
			{
				"native": true,
				"command": "workbench.action.nextSideBarView",
				"key": "alt+]",
				"when": "sideBarFocus"
			},
			{
				"native": true,
				"command": "workbench.action.previousSideBarView",
				"key": "alt+[",
				"when": "sideBarFocus"
			},
			{
				"native": true,
				"command": "actions.find",
				"key": "ctrl+alt+f"
			},
			{
				"native": true,
				"command": "editor.action.smartSelect.expand",
				"key": "ctrl+h",
				"when": "editorTextFocus"
			},
			{
				"native": true,
				"command": "editor.action.smartSelect.shrink",
				"key": "alt+h",
				"when": "editorTextFocus"
			},
			{
				"native": true,
				"command": "editor.action.moveLinesDownAction",
				"key": "ctrl+shift+n",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"native": true,
				"command": "editor.action.moveLinesUpAction",
				"key": "ctrl+shift+p",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"native": true,
				"command": "editor.action.copyLinesDownAction",
				"key": "alt+shift+n",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"native": true,
				"command": "editor.action.copyLinesUpAction",
				"key": "alt+shift+p",
				"when": "editorTextFocus && !editorReadonly"
			},
			{
				"native": true,
				"command": "selectNextSuggestion",
				"key": "ctrl+n",
				"when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
			},
			{
				"native": true,
				"command": "selectPrevSuggestion",
				"key": "ctrl+p",
				"when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
			},
			{
				"native": true,
				"command": "editor.action.triggerSuggest",
				"key": "alt+'",
				"when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
			},
			{
				"native": true,
				"command": "workbench.action.quickOpenNavigateNext",
				"key": "ctrl+n",
				"when": "inQuickOpen"
			},
			{
				"native": true,
				"command": "workbench.action.quickOpenNavigatePrevious",
				"key": "ctrl+p",
				"when": "inQuickOpen"
			},
			{
				"native": true,
				"command": "list.focusDown",
				"key": "ctrl+n",
				"when": "listFocus && !inputFocus"
			},
			{
				"native": true,
				"command": "list.focusUp",
				"key": "ctrl+p",
				"when": "listFocus && !inputFocus"
			},
			{
				"native": true,
				"command": "list.collapse",
				"key": "ctrl+b",
				"when": "listFocus && !inputFocus"
			},
			{
				"native": true,
				"command": "list.expand",
				"key": "ctrl+f",
				"when": "listFocus && !inputFocus"
			},
			{
				"native": true,
				"command": "list.select",
				"key": "ctrl+o",
				"when": "listFocus && !inputFocus"
			},
			{
				"native": true,
				"command": "workbench.action.focusActiveEditorGroup",
				"key": "alt+0"
			},
			{
				"native": true,
				"command": "workbench.action.togglePanel",
				"key": "alt+9"
			},
			{
				"native": true,
				"command": "workbench.action.toggleSidebarVisibility",
				"key": "alt+8"
			},
			{
				"native": true,
				"command": "workbench.action.toggleZenMode",
				"key": "alt+enter"
			},
			{
				"native": true,
				"command": "editor.action.clipboardPasteAction",
				"key": "ctrl+shift+v",
				"when": "textInputFocus && !editorReadonly"
			}
		],
		"configuration": [
			{
				"type": "object",
				"title": "emacsx configuration",
				"properties": {
					"emacsx.stepSize": {
						"type": "number",
						"default": 4,
						"description": "C-u default size"
					},
					"emacsx.killRingSize":{
						"type": "number",
						"default": 20,
						"description": "kill ring buffer size"
					},
					"emacsx.markRingSize": {
						"type": "number",
						"default": 20,
						"description": "mark ring buffer size"
					},
					"emacsx.enableBinaryTargetLine": {
						"type": "boolean",
						"default": false,
						"description": "trail `j`, `k`, `l` for some motion command"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"dependencies": {
		"lodash": "^4.17.11"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"gulp": "4.0.2",
		"gulp-typescript": "5.0.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42",
		"@types/lodash": "^4.14.134"
	}
}
